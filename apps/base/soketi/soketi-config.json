{
  "adapter": {
    "driver": "redis",
    "redis": {
      "prefix": "",
      "requestsTimeout": 5000,
      "clusterMode": false
    }
  },
  "appManager": {
    "driver": "array",
    "cache": {
      "enabled": false,
      "ttl": -1
    },
    "array": {
      "apps": [
        {
          "id": 1,
          "key": "${RFSPAGER_SOKETI_APP_KEY}",
          "secret": "${RFSPAGER_SOKETI_APP_SECRET}",
          "maxConnections": -1,
          "enableClientMessages": false,
          "enabled": true,
          "maxBackendEventsPerSecond": -1,
          "maxClientEventsPerSecond": -1,
          "maxReadRequestsPerSecond": -1,
          "webhooks": []
        }
      ]
    }
  },
  "cache": {
    "driver": "memory",
    "redis": {
      "redisOptions": {},
      "redisOptions": {},
      "clusterMode": false
    }
  },
  "channelLimits": {
    "maxNameLength": 200,
    "cacheTtl": 3600
  },
  "database": {
    "redis": {
      "host": "redis-master",
      "port": 6379,
      "db": 0,
      "username": null,
      "password": "password",
      "keyPrefix": "",
      "sentinels": null,
      "sentinelPassword": null,
      "name": "redis",
      "clusterNodes": []
    }
  },
  "debug": 0,
  "metrics": {
    "enabled": true,
    "driver": "prometheus",
    "prometheus": {
      "prefix": "soketi_"
    },
    "port": 9601
  },
  "mode": "full",
  "port": 6001
}